##LUAS ASAL
<tr><td>Unit Luas Asal</td><td>:</td>
<td>
<select  class="texts" style="width:500px" 
onchange="if ( this.value == '${tanah.unitLuasLot.id}-${tanah.unitLuasLot.keterangan}') { unitLuasLot(this.value);  } else { unitLuasLot(this.value); }">
<option value="">SILA PILIH</option>
#foreach ( $u in $unitLuas_list )
<option value="${u.id}-${u.keterangan}" #if ( $tanah.unitLuasLot.id == $u.id ) selected #end ( $>$u.kod - $u.keterangan</option>
#end
</select>
</td>
</tr>


<tr><td>Luas Asal</td><td>:</td>
<td>

<table><tr><td>
<span id="div_luas_asal">#parse("$viewPath/vm/luas_asal.vm")</span>
<input type="button" value="Convert &gt;" onclick="doDivAjaxCall3$formname('div_luas_asal_meter_sq','convert_luas_asal','');">
</td><td>
#set ( $luas_asal_meter_sq = $tanah.luasLot )
<span id="div_luas_asal_meter_sq">#parse("$viewPath/vm/luas_asal_meter_sq.vm")</span>
</td></tr></table>

</td>
</tr>


</table>
