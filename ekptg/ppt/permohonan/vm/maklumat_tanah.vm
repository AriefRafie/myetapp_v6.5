<fieldset>
<table width="100%">
<tr><td style="width:200px"></td><td style="width:10px"></td><td></td></tr>

<tr><td>Negeri:</td><td>:</td>
<td>
#if ( !$filterNegeri )
<select name="tanah_negeri_id" class="texts" style="width:500px" onchange="doDivAjaxCall$formname('div_tanah_daerah','tanah_list_daerah','')">
<option value="">SILA PILIH</option>
#foreach ( $n in $negeri_list )
<option value="$n.id" #if ( $tanah.negeri.id == $n.id ) selected #end>$n.kodNegeri - $n.namaNegeri</option>
#end
</select>
#else
<select disabled name="tanah_negeri_id" class="texts" style="width:500px" onchange="doDivAjaxCall$formname('div_tanah_daerah','tanah_list_daerah','')">
#foreach ( $n in $negeri_list )
<option value="$n.id" #if ( $tanah.negeri.id == $n.id ) selected #end>$n.kodNegeri - $n.namaNegeri</option>
#end
</select>
#end
</td>
</tr>

<tr><td>Daerah:</td><td>:</td>
<td>
<div id="div_tanah_daerah">
#if ( !$filterNegeri )

#if ( $tanah )
#parse("$viewPath/vm/tanah_select_daerah.vm")
#end

#else
#parse("$viewPath/vm/tanah_select_daerah.vm")
#end
</div>
</td>
</tr>

<tr><td>Mukim:</td><td>:</td>
<td>
<div id="div_tanah_mukim">
#if ( $tanah )
#parse("$viewPath/vm/tanah_select_mukim.vm")
#end
</div>
</td>
</tr>
</table>
</fieldset>

