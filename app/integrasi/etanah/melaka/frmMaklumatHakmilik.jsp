<fieldset>
<legend><strong>MAKLUMAT HAKMILIK</strong></legend>
<table width="100%" border="0" cellspacing="2" cellpadding="2">
  <tr>
    <td width="50%" valign="top"><table width="100%"  cellpadding="2" cellspacing="2" border="0">
        <tr>
          <td width="37%" align="right">ID HAKMILIK :</td>
          <td width="63%"><span class="style1">$!hakmilik.idHakmilik</span></td>
        </tr>
        <tr>
          <td align="right">ID JENIS HAKMILIK :</td>
          <td><span class="style1">$!hakmilik.idJenisHakmilik.toUpperCase()</span></td>
        </tr>
        <tr>
          <td align="right">NO. HAKMILIK :</td>
          <td><span class="style1">$!hakmilik.noHakmilik</span></td>
        </tr>
        <tr>
          <td align="right">NO. LOT :</td>
          <td><span class="style1">$!hakmilik.kodNoLot.toUpperCase() $!hakmilik.noPTNoLot</span></td>
        </tr>
        <tr>
          <td align="right">LUAS  :</td>
          <td><span class="style1">$!hakmilik.luas $!hakmilik.unitLuas.toUpperCase()</span></td>
        </tr>
        <tr>
          <td align="right">MUKIM :</td>
          <td><span class="style1">$!hakmilik.idMukim.toUpperCase()</span></td>
        </tr>
        <tr>
          <td align="right">DAERAH :</td>
          <td><span class="style1">$!hakmilik.idDaerah.toUpperCase()</span></td>
        </tr>
        <tr>
          <td align="right">NEGERI :</td>
          <td><span class="style1">$!hakmilik.idNegeri.toUpperCase()</span></td>
        </tr>
        <tr>
          <td align="right">TARIKH DAFTAR  :</td>
          <td><span class="style1">$!hakmilik.tarikhDaftarHakmilik.toUpperCase()</span></td>
        </tr>
      </table></td>
    <td width="50%" valign="top"><table width="100%"  cellpadding="2" cellspacing="2" border="0">
        <tr>
          <td width="37%" align="right" valign="top">KATEGORI TANAH :</td>
          <td width="63%" valign="top"><span class="style1">$!hakmilik.idKategoriTanah.toUpperCase()</span></td>
        </tr>
        <tr>
          <td align="right" valign="top">STATUS PEMILIKAN:</td>
          <td><span class="style1">$!hakmilik.tarafPegangan.toUpperCase()</span></td>
        </tr>
        <tr>
          <td width="37%" align="right" valign="top">SYARAT NYATA :</td>
          <td width="63%" rowspan="3" valign="top"><span class="style1">$!hakmilik.syaratNyata.toUpperCase()</span></td>
        </tr>
        <tr>
          <td align="right">&nbsp;</td>
        </tr>
        <tr>
          <td align="right">&nbsp;</td>
        </tr>
        <tr>
          <td align="right" valign="top">SEKATAN  :</td>
          <td rowspan="3" valign="top"><span class="style1">$!hakmilik.sekatan.toUpperCase()</span><span class="style1"></span></td>
        </tr>
        <tr>
          <td align="right">&nbsp;</td>
        </tr>
        <tr>
          <td align="right">&nbsp;</td>
        </tr>
        <tr>
          <td align="right">STATUS HAKMILIK :</td>
          <td><span class="style1">$!hakmilik.statusHakmilik.toUpperCase()</span></td>
        </tr>
      </table></td>
  </tr>
</table>
</fieldset>
