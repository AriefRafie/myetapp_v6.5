      <td width="1%" align="center" valign="top">:</td>
      <td width="34%" valign="top"><input name="HAK_NILAITARIKHMOHON" type="text" id="HAK_NILAITARIKHMOHON" value="$!HAK_NILAITARIKHMOHON" size="20" maxlength="15" $READONLY_JPPH style="text-transform:uppercase" /></td>
