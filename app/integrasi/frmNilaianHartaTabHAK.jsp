      <td width="1%" align="center" valign="top" nowrap="nowrap">:</td>
      <td width="34%" align="left" valign="top" nowrap="nowrap"><input name="HAK_NOCASIS" type="text" id="HAK_NOCASIS" style="text-transform:uppercase" onkeyup="this.value=this.value.replace(' ', '');" value="$!HAK_NOCASIS" size="50" maxlength="255" $READONLY_JKPTG /></td>
    </tr>
    <tr>
      <td width="15%" align="right" valign="top" nowrap="nowrap"><strong>BUATAN</strong></td>
      <td width="1%" align="center" valign="top" nowrap="nowrap">:</td>
      <td width="34%" align="left" valign="top" nowrap="nowrap">$selectHAKBuatan</td>
      <td width="15%" align="right" valign="top" nowrap="nowrap"><strong>BAHAN BAKAR</strong></td>
      <td width="1%" align="center" valign="top" nowrap="nowrap">:</td>
      <td width="34%" align="left" valign="top" nowrap="nowrap">$selectHAKBahanBakar</td>
    </tr>
    <tr>
      <td width="15%" align="right" valign="top" nowrap="nowrap"><strong>NAMA MODEL</strong></td>
      <td width="1%" align="center" valign="top" nowrap="nowrap">:</td>
      <td width="34%" align="left" valign="top" nowrap="nowrap"><input name="HAK_NAMAMODEL" type="text" id="HAK_NAMAMODEL" style="text-transform:uppercase" value="$!HAK_NAMAMODEL" size="50" maxlength="255" $READONLY_JKPTG /></td>
      <td width="15%" rowspan="3" align="right" valign="top" nowrap="nowrap"><strong>CATATAN</strong></td>
      <td width="1%" rowspan="3" align="center" valign="top" nowrap="nowrap">:</td>
      <td width="34%" rowspan="3" align="left" valign="top" nowrap="nowrap"><textarea name="HAK_CATATAN" cols="50" rows="4" id="HAK_CATATAN" style="text-transform:uppercase" $READONLY_JKPTG>$!HAK_CATATAN</textarea></td>
    </tr>
    <tr>
      <td width="15%" align="right" valign="top" nowrap="nowrap"><strong>KEUPAYAAN ENJIN</strong></td>
      <td width="1%" align="center" valign="top" nowrap="nowrap">:</td>
      <td width="34%" align="left" valign="top" nowrap="nowrap"><input name="HAK_KEUPAYAANENJIN" type="text" id="HAK_KEUPAYAANENJIN" style="text-transform:uppercase" value="$!HAK_KEUPAYAANENJIN" size="50" maxlength="255" $READONLY_JKPTG /></td>
    </tr>
    <tr>
      <td align="right" valign="top" nowrap="nowrap"><strong>TAHUN DIBUAT</strong></td>
      <td align="center" valign="top" nowrap="nowrap">:</td>
      <td align="left" valign="top" nowrap="nowrap"><input name="HAK_TAHUNDIBUAT" type="text" id="HAK_TAHUNDIBUAT" style="text-transform:uppercase" value="$!HAK_TAHUNDIBUAT" size="10" maxlength="4" $READONLY_JKPTG /></td>
    </tr>
    <tr>
      <td width="15%" align="right" valign="top" nowrap="nowrap">&nbsp;</td>
      <td width="1%" align="center" valign="top" nowrap="nowrap">&nbsp;</td>
      <td width="34%" align="left" valign="top" nowrap="nowrap">&nbsp;</td>
      <td align="right" valign="top" nowrap="nowrap"><strong>NAMA PEGAWAI JKPTG</strong></td>
      <td align="center" valign="top" nowrap="nowrap">:</td>
      <td align="left" valign="top" nowrap="nowrap"><input name="HAK_NAMAPEGAWAIJKPTG" type="text" id="HAK_NAMAPEGAWAIJKPTG" style="text-transform:uppercase" value="$!HAK_NAMAPEGAWAIJKPTG" size="50" maxlength="255" $READONLY_JKPTG /></td>
    </tr>
  </table>  
</fieldset>
