      <td width="34%" align="left" valign="top">
        <input name="HAK_TARIKHNILAIAN" type="text" id="HAK_TARIKHNILAIAN" value="$!HAK_TARIKHNILAIAN" size="15" maxlength="10" $READONLY_JPPH style="text-transform:uppercase" />
        <a href="javascript:displayDatePicker('HAK_TARIKHNILAIAN',false,'dmy');"><img src="../img/calendar.gif" alt="" border="0" $READONLY_PAJAKAN /></a>
      </td>
