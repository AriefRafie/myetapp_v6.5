      <td width="15%" align="right" valign="top" nowrap="nowrap"><strong>NO ENJIN</strong></td>
      <td width="1%" align="center" valign="top" nowrap="nowrap">:</td>
      <td width="34%" align="left" valign="top" nowrap="nowrap"><input name="HAK_NOENJIN" type="text" id="HAK_NOENJIN" style="text-transform:uppercase" onkeyup="this.value=this.value.replace(' ', '');" value="$!HAK_NOENJIN" size="50" maxlength="255" $READONLY_JKPTG /></td>
    </tr>
    <tr>
      <td width="15%" align="right" valign="top" nowrap="nowrap"><strong>JENIS BADAN</strong></td>
