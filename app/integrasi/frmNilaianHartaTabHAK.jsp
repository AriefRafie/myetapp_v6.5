    </tr>
    <tr>
      <td width="15%" align="right" valign="top">Nama Pegawai</td>
      <td width="1%" align="center" valign="top">:</td>
      <td width="34%" valign="top"><input name="HAK_NAMAPEGAWAI" type="text" id="HAK_NAMAPEGAWAI" value="$!HAK_NAMAPEGAWAI" size="50" maxlength="255" $READONLY_JPPH style="text-transform:uppercase" /></td>
    </tr>
    <tr>
      <td width="15%" align="right" valign="top">Cawangan JPPH</td>
