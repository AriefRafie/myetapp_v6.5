<script>
function submitRegister(s) {
	document.f.command.value = s;
	document.f.action = "";  
	document.f.submit(); 
}   
function getPage(k) {
	document.f.submission.value = "getResult";
	document.f.pageNum.value = k;
	document.f.action = "";  
	document.f.submit(); 
}
function getData(x) {
	document.f.submission.value = "getResult";
	document.f.pageNum.value = '$!current_page_num';
	document.f.dataRecord.value = x;
	document.f.action = "";  
	document.f.submit(); 
}
</script>