	<!-- NotifikasiLesenTamatTempohJob START -->
	<!-- It will run daily on 1.00 AM -->
	<schedule>
		<job>
			<name>NotifikasiLesenTamatTempohJob</name>
			<job-class>ekptg.scheduler.MON.NotifikasiLesenTamatTempohJob</job-class>
		</job>
		<trigger>
			<cron>
				<name>NotifikasiLesenTamatTempohJob</name>
				<job-name>NotifikasiLesenTamatTempohJob</job-name>				
				<cron-expression>0 0 1 1/1 * ? *</cron-expression> 
			</cron>
		</trigger>
	</schedule>
	<!-- NotifikasiLesenTamatTempohJob END -->
	
