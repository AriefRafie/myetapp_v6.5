	<!-- PHP START -->
	
	<!-- GenerateSewaBulananPenyewaanJob START -->
	<!-- It will run monthly on 1.00 AM -->
	<schedule>
		<job>
			<name>GenerateSewaBulananPenyewaanJob</name>
			<job-class>ekptg.scheduler.PHP.GenerateSewaBulananPenyewaanJob</job-class>
		</job>
		<trigger>
			<cron>
				<name>GenerateSewaBulananPenyewaanJob</name>
				<job-name>GenerateSewaBulananPenyewaanJob</job-name>				
				<cron-expression>0 0 1 1 1/1 ? *</cron-expression> 
